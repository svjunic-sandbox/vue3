{
  "presets": ["@babel/preset-env"],
  "env": {
    "development": {
      "presets": [
        [
          "@babel/preset-env",
          {
            "loose": true,
            "modules": false,
            "targets": {
              "node": "current",
              "browsers": [
                "last 2 Chrome versions",
                "last 2 Edge versions",
                "Explorer >= 11",
                "last 2 Firefox versions",
                "last 2 Safari versions",
                "iOS >= 11",
                "Android >= 6",
                "last 2 ChromeAndroid versions"
              ]
            },
            "useBuiltIns": "usage",
            "corejs": { version: 3, proposals: true },
            "debug":true      
          }
        ],
        "@babel/preset-typescript"
      ],
      "plugins": [
        "@babel/plugin-proposal-class-properties"
      ]
    },
    "production": {
      "presets": [
        [
          "@babel/preset-env",
          {
            "loose": true,
            "modules": false,
            "targets": {
              "node": "current",
              "browsers": [
                "last 2 Chrome versions",
                "last 2 Edge versions",
                "Explorer >= 11",
                "last 2 Firefox versions",
                "last 2 Safari versions",
                "iOS >= 11",
                "Android >= 6",
                "last 2 ChromeAndroid versions"
              ]
            },
            "useBuiltIns": "usage",
            "corejs": { version: 3, proposals: true },
            "debug":true      
          }
        ],
        "@babel/preset-typescript"
      ],
      "plugins": [
        "@babel/plugin-proposal-class-properties"
      ]
    }
  }
}
